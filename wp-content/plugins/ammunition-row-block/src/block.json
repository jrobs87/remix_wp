{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/ammunition-row-block",
	"version": "0.1.0",
	"title": "Ammunition Row Block",
	"category": "widgets",
	"icon": "superhero-alt",
	"description": "Create an ammunition row.",
	"supports": {
		"html": false,
    "color": {
      "background": true,
      "text": true
    },
    "align": ["wide", "full"],
    "spacing": {
      "margin": ["top", "bottom"],
      "padding": true,
      "blockGap": ["horizontal", "vertical"]
    }
	},
	"textdomain": "ammunition-row-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  "attributes": {
    "paddingTop": {
      "type": "string",
      "default": ""
    },
    "paddingBottom": {
      "type": "string",
      "default": ""
    },
    "rowClass": {
      "type": "string",
      "default": ""
    },
    "isOnDarkBg": {
      "type": "boolean",
      "default": false
    }
  }
}
